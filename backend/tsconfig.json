{
  "compilerOptions": {
    "types": ["node"],
    "target": "es6",
    "module": "commonjs",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "noImplicitAny": false,
    "outDir": "build"
  },
  "include": ["src", "prisma"]
}
